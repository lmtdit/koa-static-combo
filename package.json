{
  "name": "koa-static-combo",
  "version": "0.1.1",
  "description": "A static files combo server for Koa",
  "main": "./lib",
  "scripts": {
    "autod": "autod",
    "watch": "fis3 release -wLd lib",
    "clean": "[ -d lib ] && rm -r lib || true",
    "build": "npm run clean; fis3 release -cud lib",
    "lint": "eslint src test bin",
    "test": "npm run lint && node test/test.js",
    "prepublish": "npm run build"
  },
  "author": "lmtdit",
  "license": "MIT",
  "dependencies": {
    "babel-core": "~6.17.0",
    "babel-register": "~6.16.3",
    "fis3": "~3.4.24",
    "lru-cache": "~4.0.1"
  },
  "devDependencies": {
    "autod": "^2.7.1",
    "babel-eslint": "^7.0.0",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-arrow-functions": "^6.8.0",
    "babel-plugin-transform-es2015-for-of": "^6.8.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.16.0",
    "babel-plugin-transform-es2015-parameters": "^6.17.0",
    "babel-plugin-transform-es2015-spread": "^6.8.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "cp": "^0.2.0",
    "eslint": "^3.7.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^2.0.0",
    "koa": "^1.2.4",
    "mocha": "^3.1.0",
    "mocha-phantomjs": "^4.1.0",
    "openurl": "^1.1.1",
    "phantomjs": "^2.1.7"
  }
}
